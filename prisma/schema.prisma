generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model NGO {
  id          String @id @default(cuid())
  name        String @unique
  description String
  img_url     String
}

model Keyword {
  keyword String @unique
}

model Ressource {
  url         String @unique
  description String
}

model Elements {
  AtomicNumber Int     @id
  Element      String?
  Symbol       String?
}

model TypeOfRessource {
  name String @unique
}
